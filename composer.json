{
  "name": "bahraminekoo/larauth",
  "description": "A Laravel package for authentication wizard",
  "keywords": ["laravel", "authentication", "user"],
  "license": "MIT",
  "authors": [
    {
      "name": "Hossein Bahrami Nekoo",
      "email": "ho.bahram@gmail.com",
      "homepage": "http://gamedia520.blogspot.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "5.6.*|5.7.*"
  },
  "autoload": {
    "psr-4": {
      "Bahraminekoo\\Larauth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bahraminekoo\\Larauth\\LarauthServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}